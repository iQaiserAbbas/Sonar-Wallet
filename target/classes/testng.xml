<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sonar Wallet Test Automation Test Suite" verbose="1">

	<listeners>
		<listener class-name="com.sonarwallet.qa.Analyzer.MyTransformer" />
		<listener class-name="com.qa.ExtentReportListener.ExtentReportListener" />
	</listeners>

	<test name="Sonar Wallet Test Cases">
		<classes>
			<!--<class name="com.sonarwallet.qa.testcases.SwapTest"></class>-->
